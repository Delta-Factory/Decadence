<configuration>
    <logger name="delta.cion" level="DEBUG"/>

    <logger name="org" level="WARN" />
    <logger name="ch.qos.logback" level="WARN" />

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>[ %d{HH:mm:ss} | %logger{36} | %-5level] %msg%n</pattern>
        </encoder>
    </appender>

    <root level="INFO">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>
